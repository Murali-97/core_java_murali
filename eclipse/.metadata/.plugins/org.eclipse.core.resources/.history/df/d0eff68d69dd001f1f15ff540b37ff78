package practice1;

public class primenumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=9;
		int count=0;
		for(int i=1; i<n;i++)
		{
		 if(n%i==0) {
			 
			count++;
		 }
		 if(count==2) {
			 
			 System.out.println(n +" : is not a prime number");
		 }
		 
	
		}
		 System.out.println(n +" : is a prime number");
		
	}

}
