package practice1;

public class Triangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int n=5;
		
		for(int i=1;i<n;i++) {
			for(int j=i;j<n;j++) {
				
				System.out.print(" ");
			}
			for(int k=i;k<n;k++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
